#include <iostream>

using namespace std;
int main()
{

	int N, M, num, count = 0;

	cin >> N >> M;

	num = N / M;

	if (N > (num * M)) {

		count = N - (num * M);

	}

	for (int i = 0; i < M - count; i++)
	{

		cout << num << " ";

	}
	for (int i_1 = 0; i_1 < count; i_1++)
	{

		cout << num + 1 << " ";

	}
		
}
